    <SolidColorBrush x:Key="Foreground" Color="Black"/>

    <SolidColorBrush x:Key="ButtonBorderBrush" Color="Black"/>

    <Color x:Key="ListViewSelectedColor" R="233" G="235" B="236"/>
    
