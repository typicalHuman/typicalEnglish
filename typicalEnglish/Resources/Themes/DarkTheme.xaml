<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:typicalEnglish.Resources.Themes">
    <SolidColorBrush x:Key="PanelBackground" Color="#2d2d2d"/>

    <SolidColorBrush x:Key="Foreground" Color="White"/>

    <SolidColorBrush x:Key="ButtonBorderBrush" Color="#f2f2f3"/>

    <Color x:Key="ListViewSelectedColor" R="255" G="255" B="255"/>

    <Color x:Key="CheckBoxBorderColor" R="255" G="255" B="255"/>

    <SolidColorBrush x:Key="DecksBrush" Color="#594346"/>
    <SolidColorBrush x:Key="TestBrush" Color="#212026"/>
    <SolidColorBrush x:Key="OptionsBrush" Color="#f52d0b"/>
</ResourceDictionary>